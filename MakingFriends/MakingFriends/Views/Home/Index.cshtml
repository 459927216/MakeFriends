@{
    Layout = null;
}
<!DOCTYPE HTML>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="maximum-scale=1.0,minimum-scale=1.0,user-scalable=0,width=device-width,initial-scale=1.0" />
    <meta name="format-detection" content="telephone=no,email=no,date=no,address=no">
    <title>插肩而过</title>
    <link href="~/Content/css/aui.css" rel="stylesheet" />
    <script src="~/Content/js/jquery-1.8.3.min.js"></script>  
 
</head>
<body>
    <div id="content">
    </div>

    <section>
        <div class="aui-content">
            <footer id="footer" class="aui-bar aui-bar-tab" >
                <div class="bottom-nav aui-bar-tab-item aui-active" url="/Person/List" tapmode>
                    <i class="aui-iconfont aui-icon-home"></i>
                    <div class="aui-bar-tab-label ">
                        插肩而过
                    </div>
                </div>
                <div class="bottom-nav aui-bar-tab-item" url="/Search/ConSearch" tapmode>
                    <i class="aui-iconfont aui-icon-star"></i>
                    <div class="aui-bar-tab-label">
                        搜索
                    </div>
                </div>
                <div class="bottom-nav aui-bar-tab-item" url="/Msg/Chat" tapmode>
                    <div class="aui-badge">88 </div>
                    <i class="aui-iconfont aui-icon-cart"></i>
                    <div class="aui-bar-tab-label">消息</div>
                </div>
                <div class="bottom-nav aui-bar-tab-item" url="/My/Index" tapmode>
                    <div class="aui-dot"></div>
                    <i class="aui-iconfont aui-icon-my"></i>
                    <div class="aui-bar-tab-label">
                        我
                    </div>
                </div>
            </footer>
        </div>
    </section>

</body>
</html>
<script src="~/Content/js/api.js"></script>
<script src="~/Content/js/aui-toast.js"></script>
<script src="~/Content/js/aui-sharebox.js"></script>
<script src="~/Content/js/aui-actionsheet.js"></script>
<script src="~/Content/js/base.js"></script>


<script type="text/javascript">
    $(function () {
        globaFn.jump("/Person/List");
    });
</script>